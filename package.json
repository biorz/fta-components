    "@tarojs/taro": "3.3.3",
    "@types/node": "^14.0.0",
    "@types/react": "^16.13.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3",
    "@tarojs/react": ">=3",
    "classnames": ">=2",
    "prop-types": ">=15",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0",
    "react-native": ">=0.6",
    "typescript": ">=4"
  },
