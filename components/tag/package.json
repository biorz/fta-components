{
  "name": "@fta/components-tag",
  "version": "0.9.0",
  "description": "标签组件",
  "source": "src/index.tsx",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "keywords": [
    "taro",
    "taro-ui",
    "ui"
  ],
  "license": "MIT",
  "publishConfig": {
    "tag": "next"
